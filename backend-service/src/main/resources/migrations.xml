<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="1" author="bob">
        <createTable tableName="department">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="active" type="boolean" defaultValueBoolean="true"/>
        </createTable>
    </changeSet>

    <include file="migrations/users-schema.xml" relativeToChangelogFile="true"/>
    <include file="migrations/books-schema.xml" relativeToChangelogFile="true"/>
    <include file="migrations/users-books-schema.xml" relativeToChangelogFile="true"/>
    <include file="migrations/content/example-seed-data.xml" relativeToChangelogFile="true"/>
</databaseChangeLog>